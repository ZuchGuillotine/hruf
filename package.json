{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "tsx watch server/index.ts",
    "dev:client": "vite",
    "dev:local": "./scripts/dev-local.sh",
    "build": "vite build && esbuild server/index.ts --bundle --platform=node --format=esm --outdir=dist/server --packages=external",
    "start": "NODE_ENV=production node dist/server/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "test": "jest --config=jest.config.cjs",
    "test:watch": "jest --config=jest.config.cjs --watch",
    "test:summary": "jest --config=jest.config.cjs --testPathPattern=summaryManager",
    "test:embedding": "jest --config=jest.config.cjs --testPathPattern=embeddingService",
    "test:context": "jest --config=jest.config.cjs --testPathPattern=llmContextService",
    "test:openai": "jest --config=jest.config.cjs --testPathPattern=openai",
    "test:services": "jest --config=jest.config.cjs --testPathPattern=services",
    "test:cron": "jest --config=jest.config.cjs --testPathPattern=cron",
    "test:verbose": "jest --config=jest.config.cjs --verbose",
    "test:debug": "NODE_OPTIONS=--inspect-brk jest --config=jest.config.cjs --runInBand",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "test:auth": "node scripts/test-auth.js"
  },
