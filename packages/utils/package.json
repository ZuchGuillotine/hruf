{
  "name": "@hruf/utils",
  "version": "1.0.0",
  "description": "Shared utility functions for HRUF web and mobile applications",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./date": {
      "import": "./dist/date.js",
      "types": "./dist/date.d.ts"
    },
    "./string": {
      "import": "./dist/string.js",
      "types": "./dist/string.d.ts"
    },
    "./validation": {
      "import": "./dist/validation.js",
      "types": "./dist/validation.d.ts"
    },
    "./math": {
      "import": "./dist/math.js",
      "types": "./dist/math.d.ts"
    },
    "./data-structures": {
      "import": "./dist/data-structures.js",
      "types": "./dist/data-structures.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "jest --config=jest.config.cjs",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix"
  },
  "keywords": [
    "utilities",
    "shared",
    "typescript",
    "health",
    "hruf"
  ],
  "author": "HRUF Team",
  "license": "MIT",
  "dependencies": {
    "@react-navigation/native": "^7.1.14",
    "@react-navigation/stack": "^7.4.2",
    "@tanstack/react-query": "^5.81.5",
    "clsx": "^2.1.1",
    "expo-auth-session": "^6.2.0",
    "expo-constants": "^17.1.6",
    "expo-linking": "^7.1.5",
    "expo-router": "^5.1.1",
    "expo-secure-store": "^14.2.3",
    "nativewind": "^4.1.23",
    "react-native-gesture-handler": "^2.26.0",
    "react-native-reanimated": "^3.18.0",
    "tailwind-merge": "^2.5.4",
    "victory-native": "^41.17.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/react-navigation": "^3.0.8",
    "jest": "^29.7.0",
    "tailwindcss": "^4.1.11",
    "ts-jest": "^29.2.6",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  }
}
