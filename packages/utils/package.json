{
  "name": "@hruf/utils",
  "version": "1.0.0",
  "description": "Shared utility functions for HRUF web and mobile applications",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./date": {
      "import": "./dist/date.js",
      "types": "./dist/date.d.ts"
    },
    "./string": {
      "import": "./dist/string.js",
      "types": "./dist/string.d.ts"
    },
    "./validation": {
      "import": "./dist/validation.js",
      "types": "./dist/validation.d.ts"
    },
    "./math": {
      "import": "./dist/math.js",
      "types": "./dist/math.d.ts"
    },
    "./data-structures": {
      "import": "./dist/data-structures.js",
      "types": "./dist/data-structures.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "jest --config=jest.config.cjs",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix"
  },
  "keywords": [
    "utilities",
    "shared",
    "typescript",
    "health",
    "hruf"
  ],
  "author": "HRUF Team",
  "license": "MIT",
  "dependencies": {
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.5.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.6",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  }
}